{
    "id": "github-cli",
    "version": "1.0.0",
    "name": "GitHub CLI Feature",
    "documentationURL": "https://github.com/ragsworks/devcontainer-features/tree/master/src/github-cli",
    "description": "Installs the GitHub CLI and required dependencies.",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "none"
            ],
            "default": "latest",
            "description": "Select version of GitHub CLI to install"
        },
        "installDirectlyFromGitHubRelease": {
            "type": "boolean",
            "default": true
        }
    },
    "installsAfter": [
        "ghcr.io/ragsworks/devcontainer-features/common-utils",
        "ghcr.io/ragsworks/devcontainer-features/git"
    ]
}
